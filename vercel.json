{
  "version": 2,
  "builds": [
    {
      "src": "api/news.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "node_modules/puppeteer-core/**",
          "node_modules/cheerio/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/news",
      "dest": "/api/news"
    }
  ],
  "env": {
    "PUPPETEER_EXECUTABLE_PATH": "@chromium-binary"
  }
}
